                <Button x:Name="Siguintea7" Content="Siguiente" HorizontalAlignment="Right"  VerticalAlignment="Center" Width="124" Height="26" Margin="0,0,20,0" Grid.Row="18" Click="Siguintea7_Click" Grid.Column="1"/>
            </Grid>
        </StackPanel>
    </ScrollViewer>
</Page>
